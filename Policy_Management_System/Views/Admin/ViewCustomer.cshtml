@model IEnumerable<Policy_Management_System.Models.Registration>

@{
    ViewBag.Title = "ViewCustomer";
    Layout = "~/Views/Shared/_AdminDashboard.cshtml";
}

<h2>Registered Customer</h2>


<table class="table table-bordered">
    <tr>
        <th>
            Name
        </th>
        <th>
            Mobile
        </th>
        <th>
            DateOfBirth
        </th>
        <th>
            Gender
        </th>
        <th>
            State
        </th>
        <th>
            City
        </th>
        <th>
            Address
        </th>
        <th>
            Role
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Mobile
            </td>
            <td>
                @item.DateOfBirth
            </td>
            <td>
                @item.Gender
            </td>
            <td>
                @item.State
            </td>
            <td>
                @item.City
            </td>
            <td>
                @item.Address
            </td>
            <td>
                @item.Role.Name
            </td>

        </tr>
    }

</table>

